{
    "type": "object",
    "properties": {
        "logger": {
            "type": "object",
            "default": {},
            "env": "CONFIG_SERVER_LOGGER",
            "properties": {
                "enable": {
                    "type": "boolean",
                    "default": true,
                    "env": "CONFIG_SERVER_LOGGER_ENABLE",
                    "enum": [true, false]
                },
                "timestamp": {
                    "type": "string",
                    "default": "time",
                    "env": "TEMPLATE_LOGGER_TIMESTAMP",
                    "enum": ["none", "time", "full"]
                },
                "type": {
                    "type": "boolean",
                    "default": true,
                    "env": "CONFIG_SERVER_LOGGER_TYPE",
                    "enum": [true, false]
                },
                "mode": {
                    "type": "string",
                    "default": "prod",
                    "env": "CONFIG_SERVER_LOGGER_MODE",
                    "enum": ["dev", "prod", "debug"]
                }
            },
            "required": [
                "enable",
                "mode",
                "timestamp",
                "type"
            ]
        },
        "authorization": {
            "type": "object",
            "default": {},
            "env": "CONFIG_SERVER_AUTHORIZATION",
            "properties": {
                "users": {
                    "type": "array",
                    "default": [],
                    "env": "CONFIG_SERVER_AUTHORIZATION_USERS",
                    "minItems": 0
                }
            },
            "required": [
                "users"
            ]
        },
        "api": {
            "type": "object",
            "default": {},
            "env": "CONFIG_SERVER_API",
            "properties": {
                "enable": {
                    "type": "boolean",
                    "default": false,
                    "env": "CONFIG_SERVER_API_ENABLE",
                    "enum": [true, false]
                },
                "auth": {
                    "type": "boolean",
                    "default": false,
                    "env": "CONFIG_SERVER_API_AUTH",
                    "enum": [true, false]
                },
                "listening": {
                    "type": "string",
                    "default": "*:3001",
                    "minLength": 0,
                    "maxLength": 64,
                    "env": "CONFIG_SERVER_API_LISTENING",
                    "pattern": "^([a-zA-Z]{1}[-a-zA-Z0-9.]{0,255}|[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}|\\*)\\:[0-9]{1,5}$"
                },
                "prefix": {
                    "type": "string",
                    "default": "/api",
                    "minLength": 0,
                    "maxLength": 32,
                    "env": "CONFIG_SERVER_API_PREFIX",
                    "pattern": "^(\/|)[a-zA-Z]{1}[a-zA-Z0-9\/]{0,63}$"
                },
                "proxy": {
                    "type": "boolean",
                    "default": false,
                    "env": "CONFIG_SERVER_API_PROXY",
                    "enum": [true, false]
                },
                "subdomain_offset": {
                    "type": "integer",
                    "default": 2,
                    "env": "CONFIG_SERVER_API_SUBDOMAIN_OFFSET",
                    "minimum": 0
                },
                "proxy_header": {
                    "type": "string",
                    "default": "X-Forwarded-For",
                    "env": "CONFIG_SERVER_API_PROXY_HEADER",
                    "minLength": 0,
                    "maxLength": 32
                },
                "ips_count": {
                    "type": "integer",
                    "default": 0,
                    "env": "CONFIG_SERVER_API_IPS_COUNT",
                    "minimum": 0
                },
                "env": {
                    "type": "string",
                    "default": "development",
                    "env": "CONFIG_SERVER_API_ENV",
                    "minLength": 0,
                    "maxLength": 32
                },
                "keys": {
                    "type": "array",
                    "env": "CONFIG_SERVER_API_KEYS",
                    "items": {
                        "type": "string",
                        "minLength": 1
                    },
                    "minItems": 1
                }
            },
            "required": [
                "enable",
                "auth",
                "listening",
                "prefix",
                "proxy",
                "subdomain_offset",
                "proxy_header",
                "ips_count",
                "env"
            ]
        },
        "docker_healthcheck": {
            "type": "object",
            "default": {},
            "env": "CONFIG_SERVER_DOCKER_HEALTHCHECK",
            "properties": {
                "enable": {
                    "type": "boolean",
                    "default": false,
                    "env": "CONFIG_SERVER_DOCKER_HEALTHCHECK_ENABLE",
                    "enum": [true, false]
                },
                "timeout": {
                    "type": "integer",
                    "default": 10,
                    "env": "CONFIG_SERVER_DOCKER_HEALTHCHECK_TIMEOUT",
                    "minimum": 1
                }
            },
            "required": [
                "enable",
                "timeout"
            ]
        },
        "store": {
            "type": "object",
            "default": {},
            "env": "CONFIG_SERVER_STORE",
            "properties": {
                "keys": {
                    "type": "array",
                    "default": [],
                    "env": "CONFIG_SERVER_STORE_KEYS",
                    "minItems": 0
                },
                "default_namespace": {
                    "type": "string",
                    "default": "configs",
                    "env": "CONFIG_SERVER_STORE_DEFAULT_NAMESPACE",
                    "minLength": 1,
                    "maxLength": 64
                },
                "tmp": {
                    "type": "string",
                    "default": "tmp",
                    "env": "CONFIG_SERVER_STORE_TMP",
                    "minLength": 1,
                    "maxLength": 64
                },
                "sources": {
                    "type": "array",
                    "default": [],
                    "env": "CONFIG_SERVER_STORE_SOURCES",
                    "items": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "type"
                        ]
                    },
                    "minItems": 0
                }
            },
            "required": [
                "keys",
                "default_namespace",
                "tmp",
                "sources"
            ]
        }
    },
    "required": [
        "logger",
        "authorization",
        "api",
        "docker_healthcheck"
    ]
}